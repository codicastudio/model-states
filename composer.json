{
    "name": "codica-studio/modelstates",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/modelstates",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "facade/ignition-contracts": "^1.0",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ModelStates\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ModelStates\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
